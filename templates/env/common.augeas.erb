<% if @environment and not @environment.empty? -%>
<% @environment.keys.each do |k| -%>
<% if @environment[k] == '::undef' -%>
rm /files<%= @env_file -%>/<%= k %>
<% else -%>
set /files<%= @env_file -%>/<%= k -%> <%= @environment[k] %>
set /files<%= @env_file -%>/<%= k -%>/export ""
<% end -%>
<% end -%>
<% end -%>
